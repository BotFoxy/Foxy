{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./build",
    "baseUrl": "src",
    "target": "ES2022",
    "module": "commonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "paths": {
      "*": [
        "*",
        "../parent/src/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/structures/json/*.json",
    "*.ts",
    "src/locales/**/*.json",
    "../common/utils/RestManager.ts",
    "../common/utils/logger.ts",
    "../common/utils/colors.ts",
    "../common/utils/emotes.ts",
    "../common/utils/constants.ts"
, "../../common/types/DiscordUser.ts"  ]
}